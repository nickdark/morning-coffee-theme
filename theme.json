  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "entity.name.tag",
        "variable.language",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.class",
        "keyword.other.important",
        "storage"
      ],
      "settings": {
        "foreground": "#0451A5"
      }
    },
    {
      "scope": ["string", "support.constant"],
      "settings": {
        "foreground": "#A31515"
      }
    },
    {
      "scope": [
        "support.type",
        "support.class",
        "support.variable",
        "support.function",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded",
        "meta.object-literal.key punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#404040"
      }
    },
    {
      "scope": ["entity", "meta.object-literal.key"],
      "settings": {
        "foreground": "#BF8B00"
      }
    },
    {
      "scope": ["constant.language", "constant.numeric", "constant.other"],
      "settings": {
        "foreground": "#E67E00"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#008000"
      }
